
<h1>Movies</h1>
<div class="row">
{{#each Movies}}
  <div class="col-sm-4">
    <div class="row">
      <div class="col-sm-8">
        <img src="http://img.omdbapi.com/?i={{imdbID}}&apikey=8513e0a1" class="img-responsive">
      </div>
      <div class=col-sm-4>
        <span class="glyphicon glyphicon-remove" id="{{imdbID}}"></span>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-8">
        <p>{{Title}} ({{Year}})</p>
      </div>
      <div class="col-sm-4">
        <span class="glyphicon glyphicon-eye-open"></span>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <input class="rating" data-min="0" data-max="5" data-step=".5">
      </div>
    </div>
  </div>
{{/each}}
